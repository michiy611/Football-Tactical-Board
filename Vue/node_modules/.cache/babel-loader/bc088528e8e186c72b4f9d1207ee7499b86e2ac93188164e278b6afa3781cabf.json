if (tempX > 0) this.rects[this.selectIdx].x = tempX;\n      if (tempY > 0) this.rects[this.selectIdx].y = tempY;\n      e.preventDefault();\n    }\n    // drawImage(src) {\n    //     var canvas = document.getElementById(\"canvas\")\n    //     var context = canvas.getContext('2d')\n    //     var image = new Image()\n    //     image.src = src;\n    //     image.onload = function() {\n    //         canvas.width = image.width\n    //         canvas.height = image.height\n    //         context.drawImage(image, 0, 0)\n    //     }\n\n    //     context.beginPath();\n    //     context.arc(100, 100, 50, 0, 2 * Math.PI);\n    //     context.fillStyle = \"#00c2bc\";\n    //     context.fill();\n    // }\n  }\n};","map":{"version":3,"mappings":"AA0BA,eAAe;EACbA,IAAI,EAAE,SAAS;EACfC,IAAG,GAAK;IACN,OAAO;MACLC,KAAK,EAAE,CAAC;MACRC,EAAE,EAAE,CAAC;MACLC,EAAE,EAAE,CAAC;MACLC,QAAQ,EAAE,aAAa;MACvBC,MAAM,EAAE,KAAK;MACbC,YAAY,EAAE,IAAI;MAClBC,YAAY,EAAE,IAAI;MAClBC,SAAS,EAAE,CAAC;MACZC,KAAK,EAAE,CACL;QACEC,CAAC,EAAE,GAAG;QACNC,CAAC,EAAE,GAAG;QACNC,CAAC,EAAE,EAAE;QACLC,KAAK,EAAE,MAAM;QACbC,MAAM,EAAE,OAAO;QACfC,IAAI,EAAE;MACR,CAAC,EACD;QACEL,CAAC,EAAE,GAAG;QACNC,CAAC,EAAE,GAAG;QACNC,CAAC,EAAE,EAAE;QACLC,KAAK,EAAE,KAAK;QACZC,MAAM,EAAE,OAAO;QACfC,IAAI,EAAE;MACR,CAAC;IAEL;EACF,CAAC;EACD;EACAC,OAAM,GAAK;IACTC,OAAO,CAACC,GAAG,CAAC,mBAAmB;IAC/BC,QAAQ,CAACC,gBAAgB,CAAC,SAAS,EAAE,IAAI,CAACC,OAAO;IACjDF,QAAQ,CAACC,gBAAgB,CAAC,WAAW,EAAE,IAAI,CAACE,SAAS;EACvD,CAAC;EACDC,aAAY,GAAK;IACjB;IACEN,OAAO,CAACC,GAAG,CAAC,oBAAoB;IAChCC,QAAQ,CAACK,mBAAmB,CAAC,SAAS,EAAE,IAAI,CAACH,OAAO;IACpDF,QAAQ,CAACK,mBAAmB,CAAC,WAAW,EAAE,IAAI,CAACF,SAAS;EAC1D,CAAC;EACDG,OAAO,EAAE;IAEP;IACAC,IAAG,CAAGC,CAAC,EAAEC,CAAC,EAAE;MACV,IAAI,CAACvB,MAAK,GAAI,IAAG;MACjB,IAAI,CAACG,SAAQ,GAAIoB;MACjBD,CAAC,CAACE,cAAc,EAAC;IACnB,CAAC;IACD;IACAR,OAAM,CAAGM,CAAC,EAAE;MACV,IAAI,CAACtB,MAAK,GAAI,KAAI;MAClB,IAAI,CAACC,YAAW,GAAI,IAAG;MACvB,IAAI,CAACC,YAAW,GAAI,IAAG;MACvBoB,CAAC,CAACE,cAAc,EAAC;IACnB,CAAC;IACD;IACAP,SAAQ,CAAGK,CAAC,EAAE;M