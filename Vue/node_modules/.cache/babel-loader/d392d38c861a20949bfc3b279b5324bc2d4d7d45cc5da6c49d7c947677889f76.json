{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nexport default {\n  data() {\n    return {\n      src: null,\n      thumbnails: [],\n      selected: 0,\n      time: 0,\n      isLoading: false,\n      image: []\n    };\n  },\n  methods: {\n    handleFileSelect(event) {\n      // reset data\n      this.src = null;\n      this.thumbnails = [];\n      this.selected = 0;\n\n      // varidate file\n      const file = event.target.files[0];\n      if (!file || !file.type.match('video/*')) return;\n\n      // read file\n      const reader = new FileReader();\n      reader.onload = evt => {\n        this.src = evt.target.result;\n        this.createThumbnails(this.src);\n        this.isLoading = false;\n      };\n      reader.readAsDataURL(file);\n      this.isLoading = true;\n    },\n    // 動画を停止してその時点の秒数を取得\n    setCurrentTime() {\n      const canvas = document.createElement('canvas');\n      const context = canvas.getContext('2d');\n      const video = document.getElementById(\"video\");\n      this.time = video.currentTime;\n      // console.log(Math.ceil(this.time))\n      video.pause();\n      context.drawImage(video, 0, 0, video.videoWidth, video.videoHeight);\n      this.image.push(canvas.toDataURL('image/jpeg'));\n    },\n    createThumbnails(src) {\n      const video = document.createElement('video');\n      const canvas = document.createElement('canvas');\n      const context = canvas.getContext('2d');\n\n      // set canvas\n      video.onloadeddata = () => {\n        canvas.width = video.videoWidth;\n        canvas.height = video.videoHeight;\n        video.currentTime = 0;\n      };\n\n      // console.log(video.currentTime)\n\n      // capture thumbnail\n      video.onseeked = () => {\n        if (video.currentTime < video.duration) {\n          if (Math.ceil(this.time) == video.currentTime) {\n            context.drawImage(video, 0, 0, video.videoWidth, video.videoHeight);\n            this.thumbnails.push(canvas.toDataURL('image/jpeg'));\n          }\n          video.currentTime += Math.ceil(video.duration / 4);\n        }\n      };\n\n      // set video source\n      video.src = src;\n      video.load();\n    }\n  }\n};","map":{"version":3,"mappings":";AAsBA,eAAe;EACbA,IAAI,GAAG;IACL,OAAO;MACLC,GAAG,EAAE,IAAI;MACTC,UAAU,EAAE,EAAE;MACdC,QAAQ,EAAE,CAAC;MACXC,IAAI,EAAE,CAAC;MACPC,SAAS,EAAE,KAAK;MAChBC,KAAK,EAAE;IACT;EACF,CAAC;EACDC,OAAO,EAAE;IACPC,gBAAgB,CAACC,KAAK,EAAE;MACtB;MACA,IAAI,CAACR,GAAE,GAAI,IAAG;MACd,IAAI,CAACC,UAAS,GAAI,EAAC;MACnB,IAAI,CAACC,QAAO,GAAI;;MAEhB;MACA,MAAMO,IAAG,GAAID,KAAK,CAACE,MAAM,CAACC,KAAK,CAAC,CAAC;MACjC,IAAI,CAACF,IAAG,IAAK,CAACA,IAAI,CAACG,IAAI,CAACC,KAAK,CAAC,SAAS,CAAC,EAAE;;MAE1C;MACA,MAAMC,MAAK,GAAI,IAAIC,UAAU,EAAC;MAC9BD,MAAM,CAACE,MAAK,GAAKC,GAAG,IAAK;QACvB,IAAI,CAACjB,GAAE,GAAIiB,GAAG,CAACP,MAAM,CAACQ,MAAK;QAC3B,IAAI,CAACC,gBAAgB,CAAC,IAAI,CAACnB,GAAG;QAC9B,IAAI,CAACI,SAAQ,GAAI,KAAI;MACvB;MACAU,MAAM,CAACM,aAAa,CAACX,IAAI;MACzB,IAAI,CAACL,SAAQ,GAAI,IAAG;IACtB,CAAC;IACD;IACAiB,cAAc,GAAG;MACf,MAAMC,MAAK,GAAIC,QAAQ,CAACC,aAAa,CAAC,QAAQ;MAC9C,MAAMC,OAAM,GAAIH,MAAM,CAACI,UAAU,CAAC,IAAI;MACtC,MAAMC,KAAI,GAAIJ,QAAQ,CAACK,cAAc,CAAC,OAAO;MAC7C,IAAI,CAACzB,IAAG,GAAIwB,KAAK,CAACE,WAAU;MAC5B;MACAF,KAAK,CAACG,KAAK,EAAC;MACZL,OAAO,CAACM,SAAS,CAACJ,KAAK,EAAE,CAAC,EAAE,CAAC,EAAEA,KAAK,CAACK,UAAU,EAAEL,KAAK,CAACM,WAAW;MAClE,IAAI,CAAC5B,KAAK,CAAC6B,IAAI,CAACZ,MAAM,CAACa,SAAS,CAAC,YAAY,CAAC;IAChD,CAAC;IACDhB,gBAAgB,CAACnB,GAAG,EAAE;MACpB,MAAM2B,KAAI,GAAIJ,QAAQ,CAACC,aAAa,CAAC,OAAO;MAC5C,MAAMF,MAAK,GAAIC,QAAQ,CAACC,aAAa,CAAC,QAAQ;MAC9C,MAAMC,OAAM,GAAIH,MAAM,CAACI,UAAU,CAAC,IAAI;;MAEtC;MACAC,KAAK,CAACS,YAAW,GAAI,MAAM;QACzBd,MAAM,CAACe,KAAI,GAAIV,KAAK,CAACK,UAAS;QAC9BV,MAAM,CAACgB,MAAK,GAAIX,KAAK,CAACM,WAAU;QAChCN,KAAK,CAACE,WAAU,GAAI;MACtB;;MAEA;;MAEA;MACAF,KAAK,CAACY,QAAO,GAAI,MAAM;QACrB,IAAGZ,KAAK,CAACE,WAAU,GAAIF,KAAK,CAACa,QAAO,EAAG;UACrC,IAAIC,IAAI,CAACC,IAAI,CAAC,IAAI,CAACvC,IAAI,KAAKwB,KAAK,CAACE,WAAW,EAAE;YAC7CJ,OAAO,CAACM,SAAS,CAACJ,KAAK,EAAE,CAAC,EAAE,CAAC,EAAEA,KAAK,CAACK,UAAU